{
  "name": "precious-you-backend",
  "version": "0.1.0",
  "description": "Backend for precious.you - positive push notifications app",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "node --watch --experimental-transform-types src/index.ts",
    "start": "node --experimental-transform-types src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/jwt": "^10.0.0",
    "apple-signin-auth": "^2.0.0",
    "better-sqlite3": "^12.5.0",
    "fastify": "^5.6.2",
    "firebase-admin": "^13.6.0",
    "google-auth-library": "^10.5.0",
    "node-cron": "^4.2.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "eslint": "^9.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.1.0"
  }
}
